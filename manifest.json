{
  "manifest_version": 2,

  "name": "Anotode",
  "version": "0.1",
  "description": "Anotode Browser Extension",
  "permissions": [
    "activeTab",
    "storage",
    "http://anotode.herokuapp.com/"
  ],
  "browser_action": {
    "default_title": "Anotode",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
